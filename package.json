{
  "name": "bg-night-bot",
  "version": "0.1.0-alpha.3",
  "description": "Board Game Night Discord Bot",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint:es": "npx eslint src/",
    "lint:fix": "npx eslint src/ --fix",
    "generate:proto": "protoc --plugin=protoc-gen-ts_proto=.\\node_modules\\.bin\\protoc-gen-ts_proto.cmd --ts_proto_out=./src/generated --proto_path=./src/proto ./src/proto/discord.proto --ts_proto_opt=outputServices=grpc-js,env=node,esModuleInterop=true,useOptionalNullable=true"
  },
  "keywords": [],
  "author": "wolfinj",
  "license": "ISC",
  "devDependencies": {
    "@eslint/compat": "^1.2.4",
    "@eslint/js": "^9.17.0",
    "@types/node": "^22.10.5",
    "eslint": "^9.17.0",
    "eslint-plugin-import": "^2.31.0",
    "grpc-tools": "^1.12.4",
    "grpc_tools_node_protoc_ts": "^5.3.3",
    "prettier": "^3.4.2",
    "ts-node-dev": "^2.0.0",
    "ts-proto": "^2.6.1",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.19.0"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.2.3",
    "@grpc/grpc-js": "^1.12.5",
    "@grpc/proto-loader": "^0.7.13",
    "@grpc/reflection": "^1.0.4",
    "@types/express": "^5.0.0",
    "discord.js": "^14.17.2",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "globals": "^15.14.0",
    "winston": "^3.17.0",
    "zod": "^3.24.1"
  }
}
